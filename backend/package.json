{
  "name": "backend",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "lint": "eslint . --ext .js",
    "test": "jest --watchAll",
    "test:ci": "jest --ci --collectCoverage --reporters=default",
    "test:cover": "jest --collectCoverage",
    "start": "nodemon --exec ./node_modules/.bin/babel-node -r ./node_modules/dotenv/config src/index.js",
    "build": "babel src -d dist",
    "serve": "node -r dotenv/config dist/index.js"
  },
  "author": "GabriellaK",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "mysql": "^2.18.1",
    "uuid": "^9.0.1",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.6",
    "@babel/node": "^7.8.7",
    "@babel/preset-env": "^7.9.6",
    "@types/jest": "^25.2.1",
    "babel-jest": "^26.0.1",
    "eslint": "^7.0.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jest": "^23.10.0",
    "eslint-restricted-globals": "^0.2.0",
    "jest": "^26.0.1",
    "nodemon": "^2.0.2",
    "prettier": "2.0.5",
    "supertest": "^4.0.2"
  }
